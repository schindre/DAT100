import java.util.Scanner;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class obligoppgaver {
    public static void main(String[] args) {
    oppgave4();
    oppgave5();
    
   }
    public static void oppgave4() {
    	Scanner in = new Scanner(System.in);
    	System.out.println("skriv inn din bruttoinntekt i kr:");
    	int inntekt = in.nextInt();
    	if (inntekt <= 164100) {
    		System.out.println("inntekt er for lav til å ha en sats ");
    	}
    	else if (inntekt <= 230950) {
    		System.out.println("innekten din er av trinn 1 og har en sats på 0.93%");
    	}
    	else if (inntekt <= 580651) {
    		System.out.println("innekten din er av trinn 2 og har en sats på 2.41%");
    	}
    	else if (inntekt <= 934050) {
    		System.out.println("innekten din er av trinn 3 og har en sats på 11.52%");
    	}
    	else {
    		System.out.println("innekten din er av trinn 4 og har en sats på 14.52%");
    	}
    	
    }
    public static void oppgave5() {
    	Scanner in = new Scanner(System.in);
    	boolean poengcondition = false;
    	List<Integer> Poengliste =new ArrayList<>();
    	
    	System.out.println("hvor mange elever vil du evaluere?:");
    	int elever = in.nextInt();
    	System.out.println ("skriv inn poengsummene til elevene på følgende format '50,40,30.. etc': ");
        
    	for (int i = 0; i < elever ; i++) {
            System.out.println("skriv inn karakter til elev nr " + (i + 1));
            do {
                if (in.hasNextInt()) {
                    int poengsum = in.nextInt();
                    if (poengsum >= 0 && poengsum <= 100) {
                        poengcondition = true;
                        Poengliste.add(poengsum);
                    } else {
                        System.out.println("Poengsumen ble eksisterer ikke! ver sikker på at den er i mellom 0 og hundre : ");
                    }
                } else {
                    System.out.println("skriv inn karakter til elev");
                    poengcondition = false;
                    in.nextInt();
                }
            } while (!poengcondition);
        }

     
  
        
        
        
    public void poengleser(List<Integer> List) {
    	for (int poeng : List) {
    		int elev = List.indexOf(poeng) +1;
    		System.out.println("karakter for elev nr" + elev);
    	    if (poeng <= 39) {
    		    System.out.println("karakter: F");
    	    }
    	    else if (poeng <= 49) {
    		    System.out.println("karakter: E");
    	    }
    	    else if (poeng <= 59) {
    		    System.out.println("karakter: D");
    	    }
    	    else if (poeng <= 79) {
    		    System.out.println("karakter: C");
    	    }
    	    else if (poeng <= 89) {
    		    System.out.println("karakter: B");
    	    }
    	    else {
    		    System.out.println("karakter: A");
    	}
    }
}

}    