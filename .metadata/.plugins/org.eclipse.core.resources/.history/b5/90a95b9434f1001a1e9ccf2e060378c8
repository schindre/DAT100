import java.util.Scanner;

public class obligoppgaver {
    public static void main(String[] args) {
	   oppgave4();
   }
    public static void oppgave4() {
    	Scanner in = new Scanner(System.in);
    	System.out.println("skriv inn din bruttoinntekt i kr:");
    	int inntekt = in.nextInt();
    	if (inntekt <= 164100) {
    		System.out.println("inntekt er for lav til å ha en sats ");
    	}
    	else if (inntekt <= 230950) {
    		System.out.println("innekten din er av trinn 1 og har en sats på 0.93%");
    	}
    	else if (inntekt <= 580651) {
    		System.out.println("innekten din er av trinn 2 og har en sats på 2.41%");
    	}
    	else if (inntekt <= 934050) {
    		System.out.println("innekten din er av trinn 3 og har en sats på 11.52%");
    	}
    	else {
    		System.out.println("innekten din er av trinn 4 og har en sats på 14.52%");
    	}
    	
    	}
    }


